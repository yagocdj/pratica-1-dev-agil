<div class="nested-form-wrapper" data-new-record="<%= f.object.new_record? %>">
    <%= f.label :contact_value %>
    <%= f.text_field :contact_value %>

    <%= f.label :contact_type %>
    <%= f.text_field :contact_type %>

    <button type="button" data-action="nested-form#remove">Remove contact</button>

    <%= f.hidden_field :_destroy %>
</div>